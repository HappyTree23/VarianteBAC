#include <iostream>
using namespace std;

int main() {
  int i, k, j, a[6][4];
  for (i=0; i<6; i++)
    for (j=i; j<6; j++)
      if (a[i]%2==0 && a[j]%2==0) 
        if (a[i] < a[j]) {
          k = a[i];
          a[i] = a[j];
          a[j] = k;
        }
  for (i=0; i<6; i++) {
    for (j=0; j<4; j++)
      cout << a[i][j] << " ";
    cout << endl;
  }
  return 0;
}
